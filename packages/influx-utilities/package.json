{
	"name": "@skyra/influx-utilities",
	"version": "0.0.1",
	"description": "A tiny opinionated abstraction layer for InfluxDB for interacting with a single organization",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"type": "commonjs",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"test": "eslint src --ext ts -c ../../package.json",
		"build": "tsup && tsc -b src",
		"watch": "tsup --watch --onSuccess \"tsc -b src\"",
		"watch:types": "tsc -b src -w",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepublish": "rollup-type-bundler -v -e dotenv dotenv-cra",
		"bump": "standard-version --no-verify",
		"changelog": "git cliff --prepend ./CHANGELOG.md -l -c ./cliff.toml -r ../../ --include-path 'packages/influx-utilities/*'",
		"check-update": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/influx-utilities/*'",
		"release": "yarn bump && git push --follow-tags origin main && yarn npm publish"
	},
	"dependencies": {
		"@influxdata/influxdb-client": "^1.24.0"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.1.0",
		"tsup": "^6.7.0",
		"typescript": "^5.1.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/influx-utilities"
	},
	"files": [
		"dist/**/*.mjs*",
		"dist/**/*.d.ts"
	],
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
