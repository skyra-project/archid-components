{
	"name": "@skyra/influx-utilities",
	"version": "1.0.0",
	"description": "A tiny opinionated abstraction layer for InfluxDB for interacting with a single organization",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"exports": {
		"types": "./dist/index.d.ts",
		"default": "./dist/index.js"
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsup",
		"watch": "tsup --watch",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run"
	},
	"dependencies": {
		"@influxdata/influxdb-client": "^1.24.0"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.1.0",
		"tsup": "^6.7.0",
		"typescript": "^5.1.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/influx-utilities"
	},
	"files": ["dist"],
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
