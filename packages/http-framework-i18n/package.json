{
	"name": "@skyra/http-framework-i18n",
	"version": "1.0.0",
	"description": "An internationalization layer powered by i18next and @skyra/i18next-backend for @skyra/http-framework",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"exports": {
		"import": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsup",
		"watch": "tsup --watch",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run"
	},
	"dependencies": {
		"@discordjs/collection": "^1.5.0",
		"@sapphire/utilities": "^3.11.1",
		"@skyra/i18next-backend": "workspace:^",
		"discord-api-types": "^0.37.38",
		"i18next": "^22.4.14",
		"tslib": "^2.5.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/http-framework-i18n"
	},
	"files": [
		"dist/"
	],
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.0.0",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4"
	}
}
