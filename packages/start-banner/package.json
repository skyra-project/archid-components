{
	"name": "@skyra/start-banner",
	"version": "0.0.1",
	"description": "A banner generator utility for your CLI",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"sideEffects": false,
	"scripts": {
		"test": "jest",
		"build": "tsup && tsc -b src",
		"watch": "tsup --watch --onSuccess \"tsc -b src\"",
		"watch:types": "tsc -b src -w",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepublishOnly": "yarn build",
		"bump": "standard-version",
		"changelog": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/start-banner/*'"
	},
	"standard-version": {
		"tagPrefix": "@skyra/start-banner@",
		"skip": {
			"changelog": true
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/start-banner"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
