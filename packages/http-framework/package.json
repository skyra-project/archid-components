{
	"name": "@skyra/http-framework",
	"version": "2.1.0",
	"description": "The framework for ArchId Network's HTTP-only bots",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/esm/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"type": "module",
	"exports": {
		"import": {
			"types": "./dist/esm/index.d.ts",
			"default": "./dist/esm/index.js"
		}
	},
	"sideEffects": false,
	"scripts": {
		"test": "vitest run",
		"build": "tsup",
		"watch": "tsup --watch",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run"
	},
	"dependencies": {
		"@discordjs/builders": "^1.8.2",
		"@discordjs/collection": "^1.5.3",
		"@discordjs/rest": "^2.3.0",
		"@discordjs/util": "^1.1.0",
		"@sapphire/pieces": "^4.3.1",
		"@sapphire/result": "^2.6.6",
		"@sapphire/utilities": "^3.17.0",
		"@vladfrangu/async_event_emitter": "^2.4.4",
		"discord-api-types": "^0.37.93"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^4.0.3",
		"@vitest/coverage-v8": "^2.0.4",
		"tsup": "^8.2.3",
		"typescript": "~5.4.5",
		"vitest": "^2.0.4"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/http-framework"
	},
	"files": [
		"dist/"
	],
	"engines": {
		"node": ">=18"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
