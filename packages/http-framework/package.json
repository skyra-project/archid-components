{
	"name": "@skyra/http-framework",
	"version": "0.3.4",
	"description": "The framework for ArchId Network's HTTP-only bots",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"test": "jest",
		"build": "tsup && tsc -b src",
		"watch": "tsup --watch --onSuccess \"tsc -b src\"",
		"watch:types": "tsc -b src -w",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepublishOnly": "yarn build",
		"bump": "standard-version --no-verify",
		"changelog": "git cliff --prepend ./CHANGELOG.md -l -c ./cliff.toml -r ../../ --include-path 'packages/http-framework/*'",
		"check-update": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/http-framework/*'",
		"release": "yarn bump && git push --follow-tags origin main && yarn npm publish"
	},
	"dependencies": {
		"@discordjs/builders": "0.13.0-dev.1649937819-b01f414",
		"@discordjs/collection": "^0.5.0",
		"@discordjs/rest": "0.4.0-dev.1649937819-b01f414",
		"@sapphire/pieces": "^3.3.1",
		"@sapphire/utilities": "^3.6.2",
		"discord-api-types": "^0.29.0",
		"fastify": "^3.28.0",
		"form-data": "^4.0.0",
		"tweetnacl": "^1.0.3"
	},
	"devDependencies": {
		"@types/node-fetch": "^2.6.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/http-framework"
	},
	"files": [
		"dist/**/*.mjs*",
		"dist/**/*.d.ts"
	],
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
