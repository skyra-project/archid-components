{
	"name": "@skyra/http-framework",
	"version": "0.5.0",
	"description": "The framework for ArchId Network's HTTP-only bots",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"test": "jest",
		"build": "tsup && tsc -b src",
		"watch": "tsup --watch --onSuccess \"tsc -b src\"",
		"watch:types": "tsc -b src -w",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepack": "yarn build",
		"bump": "node $(yarn bin cj)",
		"check-update": "node $(yarn bin cj) --dry-run"
	},
	"dependencies": {
		"@discordjs/builders": "0.14.0-dev.1653480270-68d5169",
		"@discordjs/collection": "0.7.0-dev.1653480253-68d5169",
		"@discordjs/rest": "0.5.0-dev.1653480262-68d5169",
		"@sapphire/pieces": "^3.3.3",
		"@sapphire/utilities": "^3.6.2",
		"discord-api-types": "0.33.0",
		"fastify": "^3.29.0",
		"form-data": "^4.0.0",
		"tweetnacl": "^1.0.3"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/http-framework"
	},
	"files": [
		"dist/**/*.mjs*",
		"dist/**/*.d.ts"
	],
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^1.8.0",
		"tsup": "^6.0.1",
		"typescript": "^4.7.2"
	},
	"volta": {
		"node": "18.2.0",
		"npm": "8.5.3"
	}
}
