{
	"name": "@skyra/http-framework",
	"version": "0.15.4",
	"description": "The framework for ArchId Network's HTTP-only bots",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"test": "vitest run",
		"build": "tsup",
		"watch": "tsup --watch",
		"typecheck": "tsc -p tsconfig.eslint.json",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepack": "yarn build",
		"bump": "cliff-jumper",
		"check-update": "cliff-jumper --dry-run"
	},
	"dependencies": {
		"@discordjs/builders": "^1.5.0",
		"@discordjs/collection": "^1.4.0",
		"@discordjs/rest": "^1.7.0",
		"@sapphire/pieces": "^3.6.1",
		"@sapphire/result": "^2.6.0",
		"@sapphire/utilities": "^3.11.0",
		"@vladfrangu/async_event_emitter": "^2.1.4",
		"discord-api-types": "^0.37.37"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^2.0.0",
		"@vitest/coverage-c8": "^0.29.8",
		"tsup": "^6.7.0",
		"typescript": "^4.9.5",
		"vitest": "^0.29.8"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/http-framework"
	},
	"files": [
		"dist/**/*.mjs*",
		"dist/**/*.d.ts"
	],
	"engines": {
		"node": ">=16.17.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
