{
	"name": "@skyra/shared-http-pieces",
	"version": "0.0.14",
	"description": "The common pieces for ArchId Network's HTTP-only bots",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js"
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsc -b src",
		"watch": "tsc -b src --watch",
		"watch:types": "tsc -b src -w",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepublishOnly": "yarn build"
	},
	"dependencies": {
		"@discordjs/builders": "0.13.0-dev.1649505810-3c0bbac",
		"@skyra/http-framework": "^0.2.2",
		"discord-api-types": "^0.29.0",
		"tslib": "^2.3.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/shared-http-pieces"
	},
	"files": [
		"dist",
		"!dist/*.tsbuildinfo"
	],
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
