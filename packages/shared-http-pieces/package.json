{
	"name": "@skyra/shared-http-pieces",
	"version": "0.0.19",
	"description": "The common pieces for ArchId Network's HTTP-only bots",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"build": "tsc -b src",
		"watch": "tsc -b src --watch",
		"watch:types": "tsc -b src -w",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepublishOnly": "yarn build",
		"bump": "standard-version --no-verify",
		"changelog": "git cliff --prepend ./CHANGELOG.md -l -c ./cliff.toml -r ../../ --include-path 'packages/shared-http-pieces/*'",
		"check-update": "git cliff --prepend ./CHANGELOG.md -u -c ./cliff.toml -r ../../ --include-path 'packages/shared-http-pieces/*'",
		"release": "yarn bump && git push --follow-tags origin main && yarn npm publish"
	},
	"dependencies": {
		"@discordjs/builders": "0.13.0-dev.1649937819-b01f414",
		"@skyra/http-framework": "workspace:^",
		"discord-api-types": "^0.29.0",
		"tslib": "^2.3.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/shared-http-pieces"
	},
	"files": [
		"dist/**/*.mjs*",
		"dist/**/*.d.ts"
	],
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	}
}
