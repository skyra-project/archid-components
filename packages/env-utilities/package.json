{
	"name": "@skyra/env-utilities",
	"version": "1.0.0",
	"description": "Functional utilities for reading and parsing environmental variables",
	"author": "@skyra",
	"license": "Apache-2.0",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		"import": "./dist/index.mjs",
		"require": "./dist/index.js",
		"types": "./dist/index.d.ts"
	},
	"sideEffects": false,
	"scripts": {
		"test": "eslint src --ext ts -c ../../package.json",
		"build": "tsup && tsc -b src",
		"watch": "tsup --watch --onSuccess \"tsc -b src\"",
		"watch:types": "tsc -b src -w",
		"lint": "eslint src --ext ts --fix -c ../../package.json",
		"prepack": "rollup-type-bundler -v -e dotenv dotenv-cra",
		"bump": "node $(yarn bin cj)",
		"check-update": "node $(yarn bin cj) --dry-run"
	},
	"dependencies": {
		"dotenv-cra": "^3.0.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/skyra-project/archid-components.git",
		"directory": "packages/env-utilities"
	},
	"files": [
		"dist/**/*.mjs*",
		"dist/**/*.d.ts"
	],
	"engines": {
		"node": ">=16.9.0",
		"npm": ">=8.0.0"
	},
	"keywords": [
		"discord",
		"api",
		"http",
		"skyra",
		"typescript",
		"ts",
		"yarn"
	],
	"bugs": {
		"url": "https://github.com/skyra-project/archid-components/issues"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@favware/cliff-jumper": "^1.8.0",
		"tsup": "^6.0.1",
		"typescript": "^4.7.2"
	},
	"volta": {
		"node": "18.2.0",
		"npm": "8.5.3"
	}
}
